    <properties key="generalProj.esf.frameworkName" value="Silicon Labs Flex SDK"/>
    <properties key="generalProj.esf.pluginId" value="sim-eeprom2"/>
    <properties key="module.IsESFPluginModule" value="true"/>
    <properties key="module.isLibraryModule" value="false"/>
    <properties key="module.isBaseModule" value="true"/>
